{
  "lockFileMaintenance": {
    "enabled": false
  },
  "configMigration": true,
  "onboarding": false,
  "rangeStrategy": "replace",
  "semanticCommits": "enabled",
  "timezone": "US/Central",
  "baseBranches": ["main"],
  "packageRules": [
    {
      "matchPackageNames": ["helm"],
      "extractVersion": "^v(?<version>.*)$",
      "schedule": ["at any time"]
    }
  ]
}
